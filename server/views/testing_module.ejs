    <script>
    
        function save_cycle(){
            let test_duration = document.getElementById("test_duration").value;
                $.ajax({
                    url:"/system/test",
                    method:"POST",
                    data:{
                        duration:test_duration
                    }
                }).fail(function(err){
                    console.log(err);
                })
        }

        function verif_duration(elem){
            if(elem.value<0){
                elem.value = 0;
            }
            console.log(elem.value);
            if(elem.value>999){
                elem.value = 999;
            }
        }

    </script>
